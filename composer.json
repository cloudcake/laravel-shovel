{
  "name": "stephenlake/laravel-shovel",
  "description": "A minimal package for shovelling data from an API to clients, for Laravel.",
  "type": "library",
  "license": "MIT",
  "authors": [{
    "name": "Stephen Lake",
    "email": "stephen@closurecode.com"
  }],
  "require": {
    "adbario/php-dot-notation": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "laravel/framework": "~5.5.0|~5.6.0|~5.7.0",
    "orchestra/testbench": "~3.4.0|~3.5.0|~3.6.0"
  },
  "autoload": {
    "psr-4": {
      "Shovel\\": "src/"
    },
    "files": [
      "src/Helpers/Shovel.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Shovel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shovel\\ShovelServiceProvider"
      ]
    }
  }
}
